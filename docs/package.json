{
  "name": "starlight-image-zoom-docs",
  "version": "0.9.0",
  "license": "MIT",
  "description": "Starlight plugin adding zoom capabilities to your documentation images.",
  "author": "HiDeoo <github@hideoo.dev> (https://hideoo.dev)",
  "type": "module",
  "scripts": {
    "dev": "astro dev",
    "dev:no-caption": "astro dev --config astro.no-caption.config.ts",
    "build": "astro build",
    "build:no-caption": "astro build --config astro.no-caption.config.ts",
    "preview": "astro preview",
    "preview:no-caption": "astro preview --config astro.no-caption.config.ts",
    "lint": "eslint . --cache --max-warnings=0"
  },
  "dependencies": {
    "@astrojs/starlight": "^0.30.2",
    "@hideoo/starlight-plugins-docs-components": "^0.3.0",
    "astro": "^5.0.9",
    "sharp": "^0.33.5",
    "starlight-image-zoom": "workspace:*"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@8.15.4",
  "private": true,
  "sideEffects": false,
  "keywords": [
    "starlight",
    "image",
    "zoom",
    "documentation",
    "image-zoom"
  ],
  "homepage": "https://github.com/HiDeoo/starlight-image-zoom",
  "repository": {
    "type": "git",
    "url": "https://github.com/HiDeoo/starlight-image-zoom.git",
    "directory": "docs"
  },
  "bugs": "https://github.com/HiDeoo/starlight-image-zoom/issues"
}
